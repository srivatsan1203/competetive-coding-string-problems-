#User function Template for python3

class Solution:
    def reverseWords(self, s):
        t=s.split('.')
        z=''
        for i in range(len(t)-1):
            z+=t[i][::-1]+'.'
        return(z+t[len(t)-1][::-1])    
            


#{ 
 # Driver Code Starts
#Initial Template for Python 3

if __name__ == '__main__':
    t = int(input())
    for _ in range(t):
        s = input().strip()
        ob = Solution()
        ans = ob.reverseWords(s)
        print(ans)
# } Driver Code Ends
